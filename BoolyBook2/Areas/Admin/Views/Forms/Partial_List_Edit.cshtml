@model IEnumerable<Category>

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Category List</h2>
        </div>
    </div>
    <br /><br />
    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    Category Name
                </th>
                <th>
                    Display Order
                </th>
                <th>
                    Date/Time
                </th>
                <th>
                    Date of Update
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td width="30%">
                        @obj.Name
                    </td>
                    <td width="15%">
                        @obj.DisplayOrder
                    </td>
                    <td width="15%">
                        @obj.CreatedDate
                    </td>
                    <td width="15%">
                        @obj.UpdateDate
                    </td>
                    <td>
                        <div class="w-75 btn-group" role="group">                            
                            <a class="btn btn-primary mx-2" id="editok_button_id" onclick="editokClick(@obj.Id)">Edit</a>
                        </div>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
<script>
